{
  "name": "qcode-website",
  "version": "3.4.0",
  "description": "Qwellcode - Multilingual Website with Dark Mode",
  "scripts": {
    "start": "serve -l 3000",
    "dev": "serve -l 3000",
    "build:css": "sass scss/style.scss css/style.css",
    "watch:css": "sass --watch scss/style.scss css/style.css",
    "build:static": "node scripts/build-static.js",
    "serve:static": "serve dist -l 3000"
  },
  "devDependencies": {
    "sass": "^1.97.1",
    "serve": "^14.2.4"
  }
}
